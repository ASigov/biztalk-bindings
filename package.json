{
  "private": true,
  "name": "biztalk-bindings",
  "version": "1.0.0",
  "description": "Web application for managing BizTalk binding files",
  "author": {
    "name": "Alexey Sigov"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ASigov/biztalk-bindings.git"
  },
  "scripts": {
    "start": "node dist/server/index.js",
    "build": "run-s build-server build-client",
    "verify": "run-s prettier lint build",
    "dev": "run-p *-watch",
    "build-server": "tsc --project tsconfig.json",
    "build-client": "webpack",
    "prettier": "prettier --check \"**/*.@(ts|tsx|js|json|html|md)\"",
    "lint": "eslint --ext .ts --ext .tsx --ext .js .",
    "build-server-watch": "tsc --watch --preserveWatchOutput --pretty --project tsconfig.json",
    "build-client-watch": "webpack --watch --color",
    "start-watch": "nodemon --inspect --watch dist/ --ext js --delay 2 dist/server/index.js"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "bs-custom-file-input": "^1.3.2",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "jquery": "^3.4.1",
    "multer": "^1.4.2",
    "node-expat": "^2.3.18",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.15.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/cors": "^2.8.5",
    "@types/debug": "^4.1.4",
    "@types/express": "^4.17.0",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/multer": "^1.3.7",
    "@types/node": "^12.6.6",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/webpack": "^4.4.35",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6"
  }
}
