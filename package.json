{
  "private": true,
  "name": "biztalk-bindings",
  "version": "1.0.0",
  "description": "Web application for managing BizTalk binding files",
  "author": {
    "name": "Alexey Sigov"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ASigov/biztalk-bindings.git"
  },
  "scripts": {
    "build-server": "tsc --project tsconfig.json",
    "build-client": "webpack",
    "dev-server": "nodemon --watch dist --exec node -- dist",
    "lint": "eslint --ext .ts --ext .tsx --ext .js .",
    "prettier": "prettier --write \"**/*.@(ts|tsx|js|json|html|md)\"",
    "prettier-check": "prettier --check \"**/*.@(ts|tsx|js|json|html|md)\"",
    "watch-server": "tsc --watch --project tsconfig.json",
    "watch-client": "webpack --watch"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@types/debug": "^4.1.4",
    "@types/express": "^4.17.0",
    "@types/node": "^12.6.6",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/webpack": "^4.4.35",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6"
  }
}
